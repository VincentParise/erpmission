
{% for mission in missions %}
    <div class="card m-1" style="width: 18rem;">
        <div class="card-body">
            <h5 class="card-title">{{ mission.title }}</h5>
            <h6 class="card-subtitle mb-2 text-muted">{{ mission.codename }}</h6>
            <p class="card-text">{{ mission.description }}</p>
            <p class="card-text"><strong>Pays :</strong> {{ mission.getPaysmission }}</p>
            <p class="card-text"><strong>Liste des agents pour contact :</strong></p>
            {% for agent in mission.getAgents %}
                <p class="card-text">Code : {{ agent.code}} - Nom : {{ agent.lastname }}</p><br/>
            {% endfor %}
        </div>
    </div>
{% endfor %}


